<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.lajotsarthou.cavalier">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_cavalier_recadrer"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cavalier">
        <activity android:name=".EditCompteActivity"></activity>
        <activity android:name=".JouvActivity" />
        <activity android:name=".FouasserieActivity" />
        <activity android:name=".EngagementActivity" />
        <activity android:name=".ProfilActivity" />
        <activity android:name=".EquideActivity" />
        <activity android:name=".ClubActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".AccueilActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>